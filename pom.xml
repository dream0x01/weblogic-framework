<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.r4v3zn</groupId>
    <artifactId>weblogic-tools</artifactId>
    <version>0.0.1</version>
    <packaging>jar</packaging>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>6</source>
                    <target>6</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <dependencies><!-- https://mvnrepository.com/artifact/org.apache.commons/commons-lang3 -->
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
            <version>3.9</version>
        </dependency>
        <!-- https://mvnrepository.com/artifact/org.jsoup/jsoup -->
        <dependency>
            <groupId>org.jsoup</groupId>
            <artifactId>jsoup</artifactId>
            <version>1.13.1</version>
        </dependency>
        <!-- https://mvnrepository.com/artifact/cn.hutool/hutool-all -->
        <dependency>
            <groupId>cn.hutool</groupId>
            <artifactId>hutool-all</artifactId>
            <version>5.2.4</version>
        </dependency>
        <!-- https://mvnrepository.com/artifact/org.javassist/javassist -->
        <dependency>
            <groupId>org.javassist</groupId>
            <artifactId>javassist</artifactId>
            <version>3.27.0-GA</version>
        </dependency>
        <!-- test weblogic dependencies -->
        <dependency>
            <groupId>com.weblogic</groupId>
            <artifactId>weblogic</artifactId>
            <version>2.2-SNAPSHOT</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/lib/12.2.1.3.0/weblogic.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.weblogic</groupId>
            <artifactId>wlfullclient</artifactId>
            <version>2.2-SNAPSHOT</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/lib/12.2.1.3.0/wlfullclient.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.bea.core.repackaged</groupId>
            <artifactId>springframework.spring</artifactId>
            <version>1.5.0.0_2-5-3</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/lib/12.2.1.3.0/com.bea.core.repackaged.springframework.spring.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>com.bea.core.repackaged</groupId>
            <artifactId>apache.commons.logging</artifactId>
            <version>1.2.4</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/lib/12.2.1.3.0/com.bea.core.repackaged.apache.commons.logging.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>coherence</groupId>
            <artifactId>coherence</artifactId>
            <version>1.5.0.0_2-5-3</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/lib/12.2.1.3.0/coherence.jar</systemPath>
        </dependency>
    </dependencies>
</project>